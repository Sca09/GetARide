<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EstimationCard">
        <attr name="image">
            <flag name="lite"       value="0"/>
            <flag name="executive"  value="1"/>
            <flag name="pet"        value="2"/>
            <flag name="delivery"   value="3"/>
            <flag name="baby"       value="4"/>
            <flag name="rickshaw"   value="5"/>
            <flag name="taxi"       value="6"/>
            <flag name="cabion"     value="7"/>
        </attr>
        <attr name="ico" format="reference" />
        <attr name="name" format="string" />
        <attr name="description" format="string" />
        <attr name="price_formatted" format="string" />
    </declare-styleable>

</resources>